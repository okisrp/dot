XF86AudioMute
	~/.scripts/volume -mute

XF86Audio{Raise,Lower}Volume
	~/.scripts/volume {-up,-down}

XF86MonBrightness{Up,Down}
	~/.scripts/brightness {-up,-down}

XF86AudioPlay
	playerctl play-pause

super + u
	"${TERMINAL}"

super + o
	"${BROWSER}"

super + i
	emacsclient -c -a emacs

super + n
	ebook-viewer
