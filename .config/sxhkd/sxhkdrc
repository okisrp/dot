XF86AudioMute
	~/.local/share/dotfiles/.scripts/volume.sh -mute

XF86Audio{Raise,Lower}Volume
	~/.local/share/dotfiles/.scripts/volume.sh {-up, -down}

XF86MonBrightness{Up,Down}
	~/.local/share/dotfiles/.scripts/brightness.sh {-up, -down}

XF86AudioPlay
	playerctl play-pause

F9
	slock

Print
	scrot -p

super + Print
	scrot -bpu

super + shift + Print
	scrot -bps

super + o
	brave

super + n
	ebook-viewer

super + i
	emacs
