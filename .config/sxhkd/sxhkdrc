XF86AudioMute
	$DF_SCRIPTS_DIR/volume -mute

XF86Audio{Raise,Lower}Volume
	$DF_SCRIPTS_DIR/volume {-up,-down}

XF86MonBrightness{Up,Down}
	$DF_SCRIPTS_DIR/brightness {-up,-down}

XF86AudioPlay
	playerctl play-pause

super + u
	$TERMINAL

super + o
	$BROWSER

super + i
	emacsclient -c -a emacs

super + n
	ebook-viewer
